{
	"rules": {
		"indent": [true, "tabs"],
		"max-line-length": [true, 120],
		"typedef-whitespace": [
		true,
		{
			"call-signature": "nospace",
			"index-signature": "nospace",
			"parameter": "nospace",
			"property-declaration": "nospace",
			"variable-declaration": "nospace"
		},
		{
			"call-signature": "nospace",
			"index-signature": "nospace",
			"parameter": "nospace",
			"property-declaration": "nospace",
			"variable-declaration": "nospace"
		}
		],
		"variable-name": [
			true,
			"ban-keywords",
			"check-format",
			"allow-leading-underscore",
			"allow-snake-case"
		],
		"whitespace": [
			true,
			"check-branch",
			"check-decl",
			"check-operator",
			"check-separator"
		]
	}
}
